<svg width="100%"
     height="100%"
     viewBox="{MIN_X} {MIN_Y} {WIDTH} {HEIGHT}"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:trimesh="http://github.com/mikedh/trimesh"
     trimesh:metadata="{metadata}"
>
  <path d="{PATH_STRING}"
        fill="none"
	stroke="black"
	stroke-width="{STROKE}"
	stroke-linejoin="round"
	stroke-linecap="round" />
</svg>
